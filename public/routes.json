{
  "routes": [
    {
      "route": "/api/*",
      "serve": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/api/*",
      "*.{css,js,jpg,png,svg,ico,json,txt}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  }
}
